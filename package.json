{
	"name": "js-form-validator",
	"private": false,
	"version": "0.0.1",
	"type": "module",
	"main": "./dist/js-form-validator.umd.cjs",
	"module": "./dist/js-form-validator.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/js-form-validator.js",
			"require": "./dist/js-form-validator.umd.cjs"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"prettier": "prettier .",
		"prettier:fix": "prettier --write .",
		"lint": "eslint . --max-warnings 0 --report-unused-disable-directives",
		"lint:fix": "eslint . --fix"
	},
	"devDependencies": {
		"@types/node": "^18.11.17",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"@vitest/coverage-c8": "^0.25.8",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "2.8.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.0",
		"vite-plugin-dts": "^1.7.1",
		"vitest": "^0.25.8"
	}
}
