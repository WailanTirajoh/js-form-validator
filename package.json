{
	"name": "js-formdata-validator",
	"private": false,
	"version": "0.2.2",
	"type": "module",
	"main": "./dist/js-formdata-validator.umd.cjs",
	"module": "./dist/js-formdata-validator.js",
	"types": "./dist/index.d.ts",
	"keywords": [
		"JS",
		"Form",
		"Validator"
	],
	"author": "Wailan Tirajoh",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/js-formdata-validator.js",
			"require": "./dist/js-formdata-validator.umd.cjs"
		}
