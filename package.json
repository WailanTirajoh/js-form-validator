		"@vitest/coverage-c8": "^0.31.0",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"husky": "^8.0.3",
		"prettier": "2.8.8",
		"typescript": "^5.0.2",
		"vite": "^4.0.0",
